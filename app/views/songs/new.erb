<form method="POST" action="/songs">
  <label> Song Name </label>
  <input id='Name' type="text" name="song_name">

  <label> Artist Name </label>
  <input id='Artist Name' type="text" name="artist_name">

  <% @genres.each do |genre| %>
  <input id="<%= genre.name %>" type="checkbox" name="genre[]" value="<%= genre.name %>"> <%= genre.name %>
  <% end %>

  <input type='submit' value="Create">
</form>

<h1>Artist Show</h1>
<h3>
   <a href="/artists/<%=@artist.slug%>"><%= @artist.name %></a>
</h3><br>
<ul>
  
  <% @artist.genres.each do |genre| %>
     <li>
    <a href="/genres/<%= genre.slug %>"> 
    <%= genre.name %></a></li>
  <% end %>

  <% @artist.songs.each do |song| %>
    <li>
    <a href="/songs/<%= song.slug %>"> 
    <%= song.name %></a></li>
  <% end %>
  
</ul>